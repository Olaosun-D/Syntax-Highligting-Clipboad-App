{
  "name": "clipboard-genius",
  "productName": "Clipboard Genius",
  "version": "1.0.0",
  "description": "Clipboard History for Developers",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . Clipboard-Genius --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-1.0.CompanyName=deeps inc --version-1.0.FileDescription=CE --version-1.0.ProductName=\"Clipboard-Genius\"",
    "package-linux": "electron-packager . Clipboard-Genius --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "Dipo Olaosun ",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^13.6.6",
    "electron-packager": "^13.1.1"
  },
  "dependencies": {
    "highlight.js": "^9.13.1",
    "list.js": "^1.5.0",
    "moment": "^2.24.0",
    "strip-indent": "^2.0.0"
  }
}
